{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "goto-raycast-better",
  "title": "goto-raycast-better",
  "description": "Goes to things via a Raycast extension, but better than the previous iteration.",
  "icon": "command-icon.png",
  "author": "pepopowitz",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Goto!",
      "description": "Go!",
      "mode": "view",
      "arguments": [
        {
          "name": "code",
          "type": "text",
          "placeholder": "where you want to go today?",
          "required": false
        },
        {
          "name": "subcode",
          "type": "text",
          "placeholder": "who's your daddy and what does he do?",
          "required": false
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.69.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
